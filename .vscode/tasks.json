{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "build-debug",
      "command": "dotnet",
      "type": "process",
      "args": [
        "build",
        "${workspaceFolder}/src/RTWeekend.csproj",
        "/property:GenerateFullPaths=true",
        "/consoleloggerparameters:NoSummary",
        "/p:Platform=x64",
        "/nologo"
      ],
      "problemMatcher": "$msCompile"
    },
    {
      "label": "build-release",
      "command": "dotnet",
      "type": "process",
      "args": [
        "build",
        "${workspaceFolder}/src/RTWeekend.csproj",
        "/property:GenerateFullPaths=true",
        "/consoleloggerparameters:NoSummary",
        "/p:Platform=x64",
        "/nologo",
        "-c=Release"
      ],
      "problemMatcher": "$msCompile"
    }
  ]
}
